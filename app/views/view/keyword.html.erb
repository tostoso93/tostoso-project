<%= javascript_include_tag 'script.js' %>
<%= stylesheet_link_tag 'style.js' %>
<%= form_tag({ :controller => 'keyword', :action => 'search'},
	{ :id => 'fm', :class => 'search' }) do %>
	保存ファイル名：→
	<%= text_field_tag 'keywd', '',{ :size => 30 } %>
	<%= submit_tag '保存'  %>
<% end %>
<div id="IDCanvasDiv">
<canvas id="canvas1" width="1000" height="500"
	onmousemove="onMouseMove(event.clientX, event.clientY)"
	onmouseout="onMouseDown(false)"
	onmouseup="onMouseDown(false)"
	onmousedown="onMouseDown(true)"></canvas><br/>
<input type="button" value="クリア" onclick="clearCanvas('canvas1')"/><br/>
